@model IEnumerable<ExcelFile>
@{Layout = null;}

<form asp-controller="ExcelFile" asp-action="Index" method="post" enctype="multipart/form-data" >
	<input type="file" name="file" />
	<hr />
	<button type="submit">Youtube Upload</button>

</form>


@if (Model.Count() > 0)
{
	<hr />
	<table cellpadding="2" cellspacing="1" border="1">
		<tr>
			<td>Id</td>
			<td>Author</td>
			<td>Type</td>
			<td>Subject</td>
			<td>Body</td>
			<td>RequestSubmissionDate</td>
			<td>RequestCompletionDate</td>
			<td>Status</td>
		</tr>
		@foreach(ExcelFile itRequest in Model)
		{
			<tr>
				<td>@itRequest.FileId</td>
				<td>@itRequest.Author</td>
				<td>@itRequest.Type</td>
				<td>@itRequest.Subject</td>
				<td>@itRequest.Body</td>
				<td>@itRequest.RequestSubmissionDate</td>
				<td>@itRequest.RequestCompletionDate</td>
				<td>@itRequest.Status</td>
			</tr>
		}
	</table>

}


